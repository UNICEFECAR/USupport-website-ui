{
  "name": "usupport-website-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5177",
    "staging": "vite build --mode staging && node scripts/generate-lang-indexes.mjs && PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true react-snap",
    "build": "vite build && node scripts/generate-lang-indexes.mjs && PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true react-snap",
    "preview": "vite preview",
    "test-snap": "npm run build && npm run preview",
    "lint": "eslint --ext .js,.jsx src",
    "lint-fix": "eslint --ext .js,.jsx src --fix"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.1",
    "@tanstack/react-query": "^4.13.0",
    "@tanstack/react-query-devtools": "^4.14.1",
    "aos": "^2.3.4",
    "axios": "^1.7.2",
    "classnames": "^2.3.2",
    "i18next": "^21.10.0",
    "pdfjs-dist": "^5.4.296",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hls-player": "^3.0.7",
    "react-i18next": "^11.18.6",
    "react-infinite-scroll-component": "^6.1.0",
    "react-pdf": "^10.2.0",
    "react-router-dom": "^6.4.1",
    "react-share": "^5.2.2",
    "react-toastify": "^11.0.5",
    "react-use-webp-support-check": "^1.0.21",
    "usupport-components-library": "file:USupport-components-library"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "puppeteer-core": "^24.36.1",
    "react-snap": "^1.23.0",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "vite": "^4.4.7"
  },
  "overrides": {
    "glob-parent": "5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "puppeteerExecutablePath": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage"
    ],
    "crawlFrom": [
      "/website/pl",
      "/website/uk"
    ],
    "include": [
      "/website/pl",
      "/website/pl/",
      "/website/pl/how-it-works",
      "/website/pl/about-us",
      "/website/pl/information-portal",
      "/website/pl/my-qa",
      "/website/pl/privacy-policy",
      "/website/pl/sos-center",
      "/website/pl/cookie-policy",
      "/website/pl/terms-of-use",
      "/website/uk",
      "/website/uk/",
      "/website/uk/how-it-works",
      "/website/uk/about-us",
      "/website/uk/information-portal",
      "/website/uk/my-qa",
      "/website/uk/privacy-policy",
      "/website/uk/sos-center",
      "/website/uk/cookie-policy",
      "/website/uk/terms-of-use"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "preloadImages": false,
    "fixWebpackChunksIssue": false,
    "puppeteerIgnoreHTTPSErrors": true
  }
}
